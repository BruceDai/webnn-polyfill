{
  "name": "webnn-polyfill",
  "version": "0.0.1",
  "description": "WebNN API polyfill",
  "main": "dist/webnn-polyfill.js",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "scripts": {
    "watch": "webpack --watch",
    "start": "webpack-dev-server --open",
    "build": "webpack",
    "build-docs": "typedoc --options typedoc.json",
    "lint": "eslint . --ext .ts",
    "format": "clang-format --glob=\"{src/**/*.ts,test/**/*.js}\" --style=file -i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webmachinelearning/webnn-polyfill.git"
  },
  "keywords": [
    "deep-learning",
    "machine-learning",
    "neural-network",
    "deep-neural-network",
    "javascript",
    "webnn"
  ],
  "authors": [
    "Ningxin Hu <ningxin.hu@intel.com>"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/webmachinelearning/webnn-polyfill/issues"
  },
  "homepage": "https://github.com/webmachinelearning/webnn-polyfill",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@tensorflow/tfjs-backend-cpu": "^2.3.0",
    "@tensorflow/tfjs-backend-webgl": "^2.3.0",
    "@tensorflow/tfjs-core": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-loader": "^8.1.0",
    "clang-format": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.3.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "portfinder": "^1.0.28",
    "ts-loader": "^8.0.2",
    "typedoc": "^0.18.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {}
}
